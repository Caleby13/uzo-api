{
  "name": "aplicativo_raphael",
  "version": "1.0.0",
  "main": "./server.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "pretest": "yarn tsc && cross-env NODE_ENV=test sequelize db:migrate",
    "test": "cross-env NODE_ENV=test jest --verbose --forceExit --detectOpenHandles",
    "posttest": "cross-env NODE_ENV=test sequelize db:migrate:undo:all && rmdir dist -r",
    "dev:server": "ts-node-dev --transpile-only --ignore-watch node_modules  src/server.ts",
    "createDb": "sequelize db:create"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/factory-girl": "^5.0.8",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/supertest": "^2.0.11",
    "cross-env": "^7.0.3",
    "crossenv": "^0.0.2-security",
    "factory-girl": "^5.0.4",
    "faker": "^5.5.3",
    "jest": "^27.1.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.15.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "sqlite3": "^5.0.2"
  }
}
